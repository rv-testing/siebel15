# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
export JAVA_HOME={{siebweb_java_path}}/

PATH=$JAVA_HOME/bin:$PATH:$HOME/.local/bin:$HOME/bin

export PATH

export ORACLE_BASE={{oracle_base}}
export ORACLE_HOME=$ORACLE_BASE/product/11.2.0/client32 
export PATH=$PATH:$ORACLE_HOME/bin:$ORACLE_HOME/bin/lib:{{ses_gtwysrvr_path}}/bin
export LD_LIBRARY_PATH=/usr/lib:$LD_LIBRARY_PATH:$ORACLE_HOME/lib


export RESOLV_MULTI=off
. {{ses_gtwysrvr_path}}/{{ses_gateway_siebenv_sh}}